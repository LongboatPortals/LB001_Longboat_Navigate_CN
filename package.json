{
    "name": "PD-Template",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vite",
        "build": "vite build",
        "test:unit": "NODE_ENV=test jest",
        "test:test1": "jest tests/unit/prw.spec.js",
        "test:test2": "jest tests/unit/cn.spec.js",
        "test:test3": "jest tests/unit/es.spec.js",
        "test:test4": "jest tests/unit/yml.spec.js",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "preview": "vite preview --port 8080",
        "test:prw": "cypress run --spec 'cypress/e2e/Localtest/prw.cy.js'",
        "test:cn": "cypress run --spec 'cypress/e2e/Localtest/cn.cy.js'",
        "test:es": "cypress run --spec 'cypress/e2e/Localtest/es.cy.js'",
        "test:prodprw": "cypress run --spec 'cypress/e2e/prw.cy.js' --config-file prodprw-cypress-config.cjs",
        "test:prodcn": "cypress run --spec 'cypress/e2e/cn.cy.js' --config-file prodcn-cypress-config.cjs",
        "test:prodes": "cypress run --spec 'cypress/e2e/es.cy.js' --config-file prodes-cypress-config.cjs"
    },
    "overrides": {
        "postcss": "^8.4.47"
    },
    "dependencies": {
        "@googlemaps/js-api-loader": "^1.16.8",
        "@intlify/unplugin-vue-i18n": "^2.0.0",
        "@tabler/icons": "^2.42.0",
        "@tabler/icons-vue": "^2.42.0",
        "@vuelidate/core": "^2.0.0",
        "@vuelidate/validators": "^2.0.0",
        "axios": "^1.7.7",
        "js-cookie": "^3.0.1",
        "slidetoggle": "^3.3.2",
        "vue": "^3.3.11",
        "vue-i18n": "^9.9.0",
        "vue-router": "^4.2.5",
        "vue3-smooth-scroll": "^0.8.1"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/preset-env": "^7.25.3",
        "@rushstack/eslint-patch": "^1.3.3",
        "@vitejs/plugin-vue": "^4.5.2",
        "@vue/cli-plugin-unit-jest": "~5.0.0",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/test-utils": "^2.4.6",
        "@vue/vue3-jest": "^27.0.0",
        "autoprefixer": "^10.4.16",
        "babel-jest": "^27.5.1",
        "babel-plugin-transform-import-meta": "^2.2.1",
        "babel-plugin-transform-vite-meta-env": "^1.0.3",
        "cypress": "^13.13.3",
        "cypress-axe": "^1.5.0",
        "eslint": "^8.49.0",
        "eslint-plugin-vue": "^9.17.0",
        "jest": "^27.5.1",
        "jest-environment-jsdom": "^27.5.1",
        "jest-transform-yaml": "^1.1.2",
        "lint-staged": "^15.2.7",
        "prettier": "^3.0.3",
        "sass": "^1.79.4",
        "tailwindcss": "^3.3.3",
        "vite": "^5.0.10",
        "yaml": "^2.5.1"
    },
    "type": "module"
}
